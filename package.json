{
  "name": "@kedoupi/cconfig",
  "version": "1.1.0",
  "description": "CConfig - simple and secure Claude API configuration management",
  "main": "bin/cconfig.js",
  "bin": {
    "cconfig": "bin/cconfig.js"
  },
  "scripts": {
    "start": "node bin/cconfig.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "echo 'Integration tests not yet implemented'",
    "test:performance": "echo 'Performance tests not yet implemented'",
    "lint": "eslint bin/",
    "lint:fix": "eslint bin/ --fix",
    "format": "prettier --write bin/ *.js *.md",
    "format:check": "prettier --check bin/ *.js *.md",
    "build": "echo 'No build step required for CLI tool'",
    "prepack": "npm run lint && npm run format:check",
    "clean": "rm -rf node_modules/ package-lock.json coverage/ .nyc_output/",
    "reset": "npm run clean && npm install",
    "size": "find bin/ -name '*.js' | xargs wc -l | tail -1",
    "docs": "jsdoc bin/ -d docs/api/",
    "prepare": "husky install"
  },
  "keywords": [
    "claude",
    "claude-api",
    "cconfig",
    "configuration-manager",
    "api-management",
    "cli",
    "anthropic",
    "ai"
  ],
  "author": "CConfig Contributors",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.7",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "benchmark": "^2.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kedoupi/cconfig.git"
  },
  "bugs": {
    "url": "https://github.com/kedoupi/cconfig/issues"
  },
  "homepage": "https://github.com/kedoupi/cconfig#readme",
  "files": [
    "bin/",
    "install.sh",
    "setup.sh",
    "cconfig.sh",
    "README.md"
  ],
  "lint-staged": {
    "*.{js,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {}
}
