{
  "name": "development",
  "displayName": "开发模式",
  "description": "针对开发环境优化的输出风格",
  "version": "1.0.0",
  "style": {
    "verbosity": "balanced",
    "codeComments": true,
    "explanations": true,
    "examples": true,
    "stepByStep": true
  },
  "formatting": {
    "useEmojis": true,
    "useBold": true,
    "useColors": true,
    "maxLineLength": 100,
    "indentSize": 2
  },
  "responses": {
    "confirmationRequired": false,
    "showProgress": true,
    "includeContext": true,
    "suggestNextSteps": true
  },
  "codeGeneration": {
    "includeComments": true,
    "includeTypeHints": true,
    "includeErrorHandling": true,
    "includeLogging": true,
    "optimizeForReadability": true,
    "includeDebugInfo": true,
    "includeTestCases": true
  },
  "development": {
    "includeDebugging": true,
    "includeProfiling": false,
    "includeTestingGuidance": true,
    "includeRefactoringTips": true,
    "includePerformanceTips": false,
    "includeSecurityTips": true
  },
  "prompts": {
    "systemPrompt": "以开发者视角提供代码和解释，包含调试技巧和最佳实践。",
    "codePrompt": "提供可运行的代码，包含调试输出和测试用例。",
    "explanationPrompt": "从开发角度解释实现思路和注意事项。"
  },
  "templates": {
    "codeBlock": "```{language}\n{code}\n```\n\n🔧 **开发说明**: {devNotes}",
    "errorMessage": "🐛 **开发错误**: {message}\n\n**调试建议**: {debugTips}",
    "successMessage": "✅ **开发完成**: {message}\n\n**测试建议**: {testSuggestions}",
    "warningMessage": "⚠️ **开发注意**: {message}\n\n**最佳实践**: {bestPractices}"
  },
  "debugging": {
    "includeConsoleLog": true,
    "includeBreakpoints": true,
    "includeStackTrace": true,
    "includeVariableInspection": true
  },
  "testing": {
    "includeUnitTests": true,
    "includeIntegrationTests": false,
    "includeTestData": true,
    "includeMockExamples": true
  },
  "filters": {
    "removeMarkdownHeaders": false,
    "removeCodeComments": false,
    "removeEmptyLines": false,
    "removeVerboseExplanations": false
  }
}